<h1>Listing contact_people</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th>Position</th>
      <th>Email</th>
      <th>Decision maker</th>
      <th>Client</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contact_people.each do |contact_person| %>
      <tr>
        <td><%= contact_person.name %></td>
        <td><%= contact_person.phone %></td>
        <td><%= contact_person.position %></td>
        <td><%= contact_person.email %></td>
        <td><%= contact_person.decision_maker %></td>
        <td><%= contact_person.client %></td>
        <td><%= link_to 'Show', contact_person %></td>
        <td><%= link_to 'Edit', edit_contact_person_path(contact_person) %></td>
        <td><%= link_to 'Destroy', contact_person, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contact person', new_contact_person_path %>
